{
  "functions": {
    "api/contact.js": {
      "runtime": "@vercel/node@3.0.7"
    }
  },
  "buildCommand": "echo 'No build needed'",
  "outputDirectory": ".",
  "installCommand": "npm install",
  "rewrites": [
    {
      "source": "/",
      "destination": "/index.html"
    },
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    }
  ]
}